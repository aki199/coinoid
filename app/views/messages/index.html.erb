<div class="row">
    <div class="col-11 col-offset-250">
        <span class="notice"><%= notice %></span>
        <span class="alert"><%= alert %></span>
        <% @messages.each do |message| %>
        <h2><%=link_to message.title, message_path(message) %></h2>
        <p class="message-posted-by">Posted <%= time_ago_in_words(message.created_at) %> ago by <%= link_to User.find(message.user_id).username, user_path(message.user_id) %> in <%= link_to Category.find(message.category_id).name, category_path(message.category_id) %> </p>
            <div class="post-nav">
                <a href="http://www.facebook.com/">Share</a>
                <a href="#">Save</a>
                <a href="#">Report</a>
            </div>
        <% end %>
        </br>
        <%= link_to "New Message", new_message_path %>
    </div>
    <div class="col-fixed-250">
        <%= render "/messages/left" %>
        <%= render "/messages/left_fav" %>
    </div>
</div>
