<% @messages.each do |message| %>
    <h2><%=link_to message.title, message_path(message) %></h2>
        <p class="message-posted-by">Posted <%= time_ago_in_words(message.created_at) %> ago by <%= User.find(message.user_id).email %> </p>
        <div class="post-nav">
            <a href="#">Share</a>
            <a href="#">Save</a>
            <a href="#">Report</a>
        </div>
<% end %>
</br>
<%= link_to "New Message", new_message_path %>